  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>
    <%= it.title %>
  </title>
  <!-- Main styles for this application-->
  <%~ include("sample/head.eta", {"remote": it.remote, "compname" : it.compname}) %>
    <%~ include("sample/prescript.eta",{"remote": it.remote, "compname" : it.compname}) %>
</head>

<body>

