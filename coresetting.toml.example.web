[webnodejs]
[webnodejs.parser]
json = { limit = "1mb" }
urlencoded = { limit = "1mb", extended = true, parameterLimit = 2000 }
raw = { limit = "10mb" }
text = { limit = "1mb" }

[webnodejs.session]
secret = "WKLOH"
path = "/index"
httpOnly = true
resave = false
saveUninitialized = false
cookie = { secure = false, maxAge = 1800000 }

[webnodejs.helmet]
contentSecurityPolicy = { directives = { "script-src" = [
  "'self'",
  "example.com",
] } }

[production]
defaulturl=""
remote = { cdn = "" }
[production.upload]
disk = 5120         # 1024 * 5000 is > 5MB save to file 
stream = 61440      # 1024 * 60000 > 60MB save as streamable
location = ""

[debug]
defaulturl=""
remote = { cdn = "https://cdn.jsdelivr.net" }
[debug.upload]
disk = 5120         # 1024 * 5000 is > 5MB save to file 
stream = 61440      # 1024 * 60000 > 60MB save as streamable
location = ""
